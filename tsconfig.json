{
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "typeRoots" : ["./node_modules/@types", "./src"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
