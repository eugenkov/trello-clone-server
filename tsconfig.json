{
  "exclude": ["node_modules", "build"],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "typeRoots" : ["./node_modules/@types", "./src"],
    "outDir": "./build",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
